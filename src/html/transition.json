{
	"Vue Transition Component": {
		"prefix": "transition",
		"body": [
			"<transition name=\"${1:feature}\">",
			"  $0",
			"</transition>"
		],
		"description": "Vue Transition Element.\n@link https://vuejs.org/v2/api/#transition"
	},
