{
	"<router-view>": {
		"prefix": "router-view",
		"body": [
			"<router-view$1></router-view>$0"
		]
	},
	"<router-view>, named": {
		"prefix": "router-view-named",
		"body": [
			"<router-view name=\"${1:default}\"></router-view>$0"
		],
		"description": "Named router views\n"
	},

	"<router-link>, literal string": {
		"prefix": "router-link",
		"body": [
			"<router-link to=\"$1\">",
			"  $2",
			"</router-link>$0"
		],
		"description": "Router link with literal string.\n"
	},

	"<router-link>": {
		"prefix": "router-link",
		"body": [
			"<router-link :to=\"$1\">",
			"  $2",
			"</router-link>$0"
		],
		"description": "Router link using `v-bind`.\n"
	},
	"<router-link>, exact": {
		"prefix": "router-link",
		"body": [
			"<router-link :to=\"$1\" exact>",
			"  $2",
			"</router-link>$0"
		]
	},

	"<router-link>, path": {
		"prefix": "router-link-path",
		"body": [
			"<router-link :to=\"{ path: ${1:'${2:pathname}'} }\">",
			"  $3",
			"</router-link>$0"
		],
		"description": "Router link to path.\n"
	},
	"<router-link>, path, params": {
		"prefix": "router-link-path-params",
		"body": [
			"<router-link :to=\"{ path: ${1:'${2:pathname}'}, params: { ${3:property}: ${4:'value'} } }\">",
			"  $5",
			"</router-link>$0"
		],
		"description": "Router link to path with params.\n"
	},
	"<router-link>, path, query": {
		"prefix": "router-link-path-query",
		"body": [
			"<router-link :to=\"{ path: ${1:'${2:pathname}'}, query: { ${3:property}: ${4:'value'} } }\">",
			"  $5",
			"</router-link>$0"
		],
		"description": "Router link to path with query.\n"
	},
	"<router-link>, path, append": {
		"prefix": "router-link-path-append",
		"body": [
			"<router-link :to=\"{ path: ${1:'${2:pathname}'} }\" append>",
			"  $3",
			"</router-link>$0"
		],
		"description": "Router link to path, append mode.\n"
	},
	"<router-link>, path, replace": {
		"prefix": "router-link-path-replace",
		"body": [
			"<router-link :to=\"{ path: ${1:'${2:pathname}'} }\" replace>",
			"  $3",
			"</router-link>$0"
		],
		"description": "Router link to path, replace mode.\n"
	},

	"<router-link>, named": {
		"prefix": "router-link-named",
		"body": [
			"<router-link :to=\"{ name: ${1:'${2:routename}'} }\">",
			"  $3",
			"</router-link>$0"
		],
		"description": "Router link to named route.\n"
	},
	"<router-link>, named, params": {
		"prefix": "router-link-named-params",
		"body": [
			"<router-link :to=\"{ name: ${1:'${2:routename}'}, params: { ${3:property}: ${4:'value'} } }\">",
			"  $5",
			"</router-link>$0"
		],
		"description": "Router link to named route with params.\n"
	},
	"<router-link>, named, query": {
		"prefix": "router-link-named-query",
		"body": [
			"<router-link :to=\"{ name: ${1:'${2:routename}'}, query: { ${3:property}: ${4:'value'} } }\">",
			"  $5",
			"</router-link>$0"
		],
		"description": "Router link to named route with query.\n"
	},
	"<router-link>, named, append": {
		"prefix": "router-link-named-append",
		"body": [
			"<router-link :to=\"{ name: ${1:'${2:pathname}'} }\" append>",
			"  $3",
			"</router-link>$0"
		],
		"description": "Router Link to named route, append mode.\n"
	},
	"<router-link>, named, replace": {
		"prefix": "router-link-named-replace",
		"body": [
			"<router-link :to=\"{ name: ${1:'${2:pathname}'} }\" replace>",
			"  $3",
			"</router-link>$0"
		],
		"description": "Router Link to named route, replace mode.\n"
	},

	"router params": {
		"prefix": "router-params",
		"body": [
			"params: {",
			"  ${1:property}: ${2:'value'},$3",
			"},"
		],
		"description": "router with params.\n"
	},
	"router query": {
		"prefix": "router-query",
		"body": [
			"query: {",
			"  ${1:property}: ${2:'value'},$3",
			"},"
		],
		"description": "router with query.\n"
	}
}
