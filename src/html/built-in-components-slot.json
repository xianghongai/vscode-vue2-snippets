{
	"<slot> Element": {
		"prefix": "slotElement",
		"body": [
			"<slot ${1:name=\"${2:default}\"}>$3</slot>$0"
		],
		"description": "`<slot>` serve as content distribution outlets in component templates. `<slot>` itself will be replaced."
	},
	"<slot> Element, Scoped Slots": {
		"prefix": "slotElementScope",
		"body": [
			"<slot ${1:name=\"${2:default}\"} :${3:slotProps}=\"$3\">$4</slot>$0"
		],
		"description": "`<slot>` serve as content distribution outlets in component templates. `<slot>` itself will be replaced.\n`:view-box.camel` is not needed if you are using string templates, or compiling with vue-loader/vueify."
	},
	"v-slot": {
		"prefix": "vSlot",
		"body": [
			"<template #${1:default}>",
			"  $0",
			"</template>"
		],
		"description": "Named Slot"
	},
	"v-slot, Scoped Slots": {
		"prefix": "vSlotScope",
		"body": [
			"<template #${1:default}=\"${2:slotProps}\">",
			"  $0",
			"</template>"
		],
		"description": "Scoped Slots"
	},
	"v-slot, Longhand": {
		"prefix": "vSlotLonghand",
		"body": [
			"<template v-slot:${1:default}>",
			"  $0",
			"</template>"
		],
		"description": "Named Slots, Longhand"
	},
	"v-slot, Scoped Slots, Longhand": {
		"prefix": "vSlotScopeLonghand",
		"body": [
			"<template v-slot:${1:default}=\"${2:slotProps}\">",
			"  $0",
			"</template>"
		],
		"description": "Named and Scoped Slots, Longhand"
	},
	"UNSAFE!!! Slot Deprecated": {
		"prefix": "UNSAFE_slot",
		"body": [
			"<template slot=\"default\">",
			"  $0",
			"</template>"
		],
		"description": "Prefer `v-slot` in 2.6.0+."
	},
	"UNSAFE!!! Slot Deprecated, Scope Removed": {
		"prefix": "UNSAFE_slotScope",
		"body": [
			"<template slot=\"default\" scope=\"slotProps\">",
			"  $0",
			"</template>"
		],
		"description": "`scope` replaced by `slot-scope` in 2.5.0+. \n`slot` prefer `v-slot` in 2.6.0+."
	},
	"UNSAFE!!! Slot Deprecated, slotScope Deprecated": {
		"prefix": "UNSAFE_slotSlotScope",
		"body": [
			"<template slot=\"default\" slot-scope=\"slotProps\">",
			"  $0",
			"</template>"
		],
		"description": "Prefer v-slot in 2.6.0+."
	}
}
