			"${1:property}: {",
			"\ttype: ${2:String|Number|Boolean|Array|Objec|Date|Function|Symbol|Promise},",
			"\trequired: ${3:boolean},",
			"\tdefault() {",
			"\t\treturn ${4:any};",
			"\t},",
			"\tvalidator(value) {",
			"\t\treturn ${5:boolean };",
			"\t},",
			"},$0"
		]
	},
