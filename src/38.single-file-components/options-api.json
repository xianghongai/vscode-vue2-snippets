			"  <div>",
			"    $1",
			"  </div>",
			"</template>",
			"",
			"<script>",
			"export default {",
			"  name: '${WORKSPACE_NAME/(.*)/${1:/pascalcase}/}${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}',",
			"",
			"  data() {",
			"    return {",
			"      ${6}",
			"    };",
			"  },$0",
			"",
			"  mounted() {",
			"    ${7}",
			"  },",
			"",
			"  methods: {",
			"    ${8}",
			"  },",
